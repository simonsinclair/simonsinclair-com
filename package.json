    "axe-core": "^4.0.2",
    "clean-css": "^4.2.3",
    "cypress": "^5.3.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "html-minifier": "^4.0.0",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
